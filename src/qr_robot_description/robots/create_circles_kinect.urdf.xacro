<?xml version="1.0"?>
<!--
    - Base      : create
    - Stacks    : circles
    - 3d Sensor : kinect
-->    
<robot name="turtlebot" xmlns:xacro="http://ros.org/wiki/xacro">

  <xacro:include filename="$(find qr_robot_description)/urdf/turtlebot_common_library.urdf.xacro" />
  <xacro:include filename="$(find create_description)/urdf/create.urdf.xacro"/>
  <xacro:include filename="$(find qr_robot_description)/urdf/stacks/circles.urdf.xacro"/>
  <xacro:include filename="$(find qr_robot_description)/urdf/sensors/kinect.urdf.xacro"/>

  <create/>
  <stack_circles parent="base_link"/>
  <sensor_kinect  parent="base_link"/>
<robot>
